{
  "name": "@ikun-kit/fepull",
  "version": "1.0.4",
  "description": "Frontend package management tool for pulling packages from monorepo projects",
  "keywords": [
    "cli",
    "frontend",
    "monorepo",
    "package",
    "tool"
  ],
  "license": "MIT",
  "author": "xiaoyown",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikun-kit/fepull.git"
  },
  "bin": {
    "fepull": "dist/bin.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "README.zh.md",
    "fepull.config.example.yml"
  ],
  "type": "module",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "dev": "tsc --watch",
    "format": "oxfmt --write",
    "format:check": "oxfmt --check",
    "lint": "pnpm lint:types && oxlint .",
    "lint:fix": "oxlint . --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "pnpm run build",
    "release": "pnpm run build && changeset publish",
    "test:install": "node dist/bin.js install"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "picocolors": "^1.0.0",
    "simple-git": "^3.20.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.8.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
