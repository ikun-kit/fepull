{
  "name": "@ikun-kit/fepull",
  "version": "1.0.2",
  "description": "Frontend package management tool for pulling packages from monorepo projects",
  "keywords": [
    "frontend",
    "package",
    "monorepo",
    "cli",
    "tool"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ikun-kit/fepull.git"
  },
  "license": "MIT",
  "author": "xiaoyown",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fepull": "dist/bin.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "README.zh.md",
    "fepull.config.example.yml"
  ],
  "scripts": {
    "build": "tsc",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "oxlint --deny-warnings && tsc --noEmit",
    "lint:fix": "oxlint --fix",
    "prepare": "pnpm run build",
    "release": "pnpm run build && changeset publish",
    "start": "node dist/bin.js"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "picocolors": "^1.0.0",
    "simple-git": "^3.20.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.8.0",
    "oxlint": "^0.11.0",
    "prettier": "^3.1.0",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-sort-json": "^3.1.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
